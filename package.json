{
  "name": "electron-to-web",
  "version": "0.1.2",
  "description": "Deploy your Electron app to the web without rewriting code - drop-in replacement for Electron's IPC using JSON-RPC over WebSocket",
  "type": "module",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "exports": {
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.js",
      "require": "./dist/main/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "ARCHITECTURE.md",
    "FEATURE_PARITY.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run test:e2e",
    "test:e2e": "npm run build && node tests/run-tests.mjs",
    "test:mocha": "npm run build && mocha tests/e2e.test.mjs",
    "test:watch": "npm run build && mocha tests/e2e.test.mjs --watch"
  },
  "keywords": [
    "electron",
    "web",
    "ipc",
    "json-rpc",
    "websocket",
    "migration",
    "browser",
    "desktop"
  ],
  "author": "electron-to-web contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsadehaan/electron-to-web.git"
  },
  "bugs": {
    "url": "https://github.com/lsadehaan/electron-to-web/issues"
  },
  "homepage": "https://github.com/lsadehaan/electron-to-web#readme",
  "dependencies": {
    "json-rpc-2.0": "^1.7.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.27",
    "@types/ws": "^8.18.1",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^5.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
